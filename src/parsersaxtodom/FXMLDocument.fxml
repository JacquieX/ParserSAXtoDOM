<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<HBox fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="parsersaxtodom.FXMLDocumentController">
   <children>
      <TextArea prefHeight="200.0" prefWidth="200.0" promptText="xmlCode" fx:id="xmlTextArea" />
      <VBox>
         <children>
            <Button mnemonicParsing="false" onAction="#uploadClick" text="Open">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#parseClick" text="Parse">
               <VBox.margin>
                  <Insets bottom="20.0" top="70.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <TextArea fx:id="parsedTextArea" prefHeight="200.0" prefWidth="200.0" promptText="parsecode" />
   </children>
</HBox>
